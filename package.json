{
  "name": "multihue",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "clipboard-copy": "^2.0.0",
    "d3-array": "^1.2.1",
    "d3-color": "^1.2.0",
    "d3-scale": "^2.0.0",
    "d3-selection": "^1.3.0",
    "d3-shape": "^1.2.0",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-resize-detector": "^2.3.0",
    "react-scripts": "1.1.4",
    "react-toolbox": "2.0.0-beta.12"
  },
  "devDependencies": {
    "gh-pages": "^1.2.0",
    "react-toolbox-themr": "^1.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "toolbox": "react-toolbox-themr",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "homepage": "http://leonelgalan.github.io/multihue",
  "reactToolbox": {
    "include": [
      "LAYOUT",
      "MENU",
      "INPUT",
      "SLIDER",
      "SWITCH",
      "SNACKBAR"
    ],
    "customProperties": {
      "color-primary": "var(--palette-indigo-500)",
      "color-primary-dark": "var(--palette-indigo-700)",
      "color-primary-light": "var(--palette-indigo-500)",
      "animation-duration": "0.3s",
      "color-accent": "var(--palette-pink-a200)",
      "color-accent-dark": "var(--palette-pink-700)",
      "color-primary-contrast": "var(--color-dark-contrast)",
      "color-accent-contrast": "var(--color-dark-contrast)",
      "drawer-mobile-width": "80%",
      "drawer-desktop-width": "30rem"
    },
    "output": "src/assets/react-toolbox"
  }
}
